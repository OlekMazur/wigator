{
	"compilerOptions": {
		"target": "esnext",
		"module": "es2015",
		"moduleResolution": "node",
		"strict": true,
		"alwaysStrict": true,
		"strictNullChecks": true,
		"sourceMap": false,
		"outDir": "output/intermediate",
		"incremental": true,
		"tsBuildInfoFile": "output/.tsbuildinfo"
	},
	"include": [
		"src"
	]
}
